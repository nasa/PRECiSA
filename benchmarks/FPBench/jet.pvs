
jet: THEORY
BEGIN
IMPORTING PRECiSA@ieee754_double

% @fp-function
jet(X1: double,X2: double | X1*X1 + 1 /= 0):double =
    LET T = (3*X1*X1 + 2*X2 - X1)
    IN
    X1 + ((2*X1*(T/(X1*X1 + 1))*
    (T/(X1*X1 + 1) - 3) + X1*X1*(4*(T/(X1*X1 + 1))-6))*
    (X1*X1 + 1) + 3*X1*X1*(T/(X1*X1 + 1)) + X1*X1*X1 + X1 +
    3*((3*X1*X1 + 2*X2 -X1)/(X1*X1 + 1)))

END jet
