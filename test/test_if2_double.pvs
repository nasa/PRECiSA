test_if2_double: THEORY
BEGIN
IMPORTING float@aerr754dp

f (X, Y: unb_double): unb_double = IF (NOT (X = Dadd(X, Y)))
                                       THEN IF (Y = X)
				            THEN (Dadd(X, Y))
					    ELSE (Dadd(Y, Y))
					    ENDIF
                                       ELSE (Dmul(X, Y))
                                   ENDIF
END test_if2_double